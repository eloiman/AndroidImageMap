<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="ImageMap">
		<attr name="map" format="string"/>
        <attr name="fitImageToScreen">
            <enum name="fitToScreen" value="0"/>
            <enum name="fitToScreenWithAspect" value="1"/>
            <enum name="rawSize" value="3"/>
        </attr>
        <attr name="scaleFromOriginal" format="boolean"/>        
        <attr name="maxSizeFactor" format="float"/>
        <attr name="nodpi" format="boolean"/>
	</declare-styleable>
</resources>